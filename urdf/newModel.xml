  <gazebo reference="wheel_middle_link">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
      <minDepth>0.001</minDepth>
      <kp>100000000</kp>
      <kd>1</kd>

      <selfCollide>false</selfCollide>
  </gazebo>

